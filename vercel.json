{
    "version": 2,
    "builds": [
      {
        "src": "client",
        "use": "@vercel/react" 
      },
      {
        "src": "server",
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "public": "true"
  }
  